def ploter(table):
    @interact(x=widgets.Dropdown(options=[None] + list(table.labels), description="Column"), value = None)
    def plot_helper(x):
        if x:
            px.histogram(table.to_df(), x = x, marginal = "box", template='seaborn', title = x).show()
